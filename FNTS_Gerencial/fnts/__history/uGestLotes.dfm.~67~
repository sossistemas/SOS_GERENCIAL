  object qryLote: TZQuery
    Connection = frmmodulo.Conexao
    SQL.Strings = (
      'select l.id,'
      '  l.descred,'
      '  l.data_inicio,'
      '  l.hora_inicio,'
      '  l.data_fim,'
      '  l.hora_fim,'
      '  l.status'
      'from lote l'
      'left join lote_item li on li.id_lote = l.id'
      'where 1 = 1'
      'and (:id is null or (l.id =:id))'
      
        'and ((:dtLini is null and :dtLfim is null) or (l.data_inicio bet' +
        'ween :dtLini and :dtLfim))'
      
        'and ((:dtVini is null and :dtVfim is null) or (l.data_fim betwee' +
        'n :dtVini and :dtVfim))'
      'and (:codp is null or (li.codproduto =:codp)) '
      'order by l.id')
    Params = <
      item
        DataType = ftUnknown
        Name = 'id'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtLini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtLfim'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVfim'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'codp'
        ParamType = ptUnknown
      end>
    Left = 360
    Top = 224
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'id'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtLini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtLfim'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVfim'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'codp'
        ParamType = ptUnknown
      end>
    object qryLoteID: TIntegerField
      FieldName = 'ID'
      Required = True
    end
    object qryLoteDESCRED: TWideStringField
      FieldName = 'DESCRED'
      Size = 40
    end
    object qryLoteDATA_INICIO: TDateField
      FieldName = 'DATA_INICIO'
    end
