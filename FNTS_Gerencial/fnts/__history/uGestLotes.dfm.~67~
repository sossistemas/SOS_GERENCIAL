  object qryItemLote: TZQuery
    Connection = frmmodulo.Conexao
    SQL.Strings = (
      'select li.*, pr.produto, pr.unidade'
      'from lote_item LI'
      'join c000025 PR on pr.codigo = li.codproduto'
      'where 1 = 1'
      'and (:codproduto is null or (li.codproduto =:codproduto))'
      'and (:idLote is null or (li.id_lote =:idlote))'
      'and (:codfor is null or (li.codfornecedor =:codfor))'
      
        'and ((:dtVini is null and :dtVfim is null) or (li.validadeprodut' +
        'o between :dtVini and :dtVfim))'
      'order by li.id_lote')
    Params = <
      item
        DataType = ftUnknown
        Name = 'codproduto'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'idLote'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'codfor'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVfim'
        ParamType = ptUnknown
      end>
    Left = 48
    Top = 464
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'codproduto'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'idLote'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'codfor'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVfim'
        ParamType = ptUnknown
      end>
    object qryItemLoteID: TIntegerField
      FieldName = 'ID'
      Required = True
    end
