  object qryFiltra: TZQuery
    Connection = frmmodulo.Conexao
    Active = True
    SQL.Strings = (
      'select VEN.*,'
      
        '       sum(((VEN.unitario * VEN.qtde) + VEN.acrescimo) - VEN.des' +
        'conto) as totalizado,'
      '   case'
      '    when coalesce(VEN.nfce,'#39'N'#39') = '#39'S'#39' then'
      
        '      (select first 1 n.numero from nfce n where n.atb like :atb' +
        'n and n.cupom = VEN.numeronota)'
      '    when coalesce(VEN.gerou_sat,'#39'N'#39') = '#39'S'#39' then'
      '      VEN.numero_sat'
      
        '    when (coalesce(VEN.nfce,'#39'N'#39') <> '#39'S'#39') and (coalesce(VEN.gerou' +
        '_sat,'#39'N'#39') <> '#39'S'#39') then'
      '      '#39#39
      '  end cupompdv,'
      
        '(select n.numero from nfce n where n.atb like :atbn and n.cupom ' +
        '= VEN.cupom_numero) as numero,'
      'ma.nome marca,'
      'fo.nome fornecedor,'
      'CLI.nome cliente,'
      'fu.nome vededor'
      'from c000032 VEN'
      'join c000025 PR on PR.codigo = VEN.codproduto'
      'join C000019 MA on MA.codigo = pr.codmarca'
      'left join c000009 FO on FO.codigo = pr.codfornecedor'
      'left join c000007 CLI on CLI.codigo = VEN.codcliente'
      'join c000008 FU on fu.codigo = VEN.codvendedor'
      'where VEN.atb like :atbc000032'
      'and VEN.movimento in (2,9, 7) and'
      'VEN.data >= :datai and VEN.data <= :dataf'
      'group by VEN.codigo,'
      '         VEN.codnota,'
      '         VEN.codproduto,'
      '         VEN.unitario,'
      '         VEN.total,'
      '         VEN.icms,'
      '         VEN.ipi,'
      '         VEN.cfop,'
      '         VEN.data,'
      '         VEN.numeronota,'
      '         VEN.codcliente,'
      '         VEN.desconto,'
      '         VEN.acrescimo,'
      '         VEN.movimento,'
      '         VEN.codvendedor,'
      '         VEN.codgrade,'
      '         VEN.serial,'
      '         VEN.unidade,'
      '         VEN.qtde,'
      '         VEN.valor_icms,'
      '         VEN.icms_reduzido,'
      '         VEN.base_calculo,'
      '         VEN.valor_ipi,'
      '         VEN.situacao,'
      '         VEN.ecf_serie,'
      '         VEN.ecf_caixa,'
      '         VEN.codaliquota,'
      '         VEN.cupom_numero,'
      '         VEN.cupom_modelo,'
      '         VEN.cupom_item,'
      '         VEN.aliquota,'
      '         VEN.cst,'
      '         VEN.lote_fabricacao,'
      '         VEN.movimento_estoque,'
      '         VEN.lancado,'
      '         VEN.vencimento,'
      '         VEN.codbarra,'
      '         VEN.margem_desconto,'
      '         VEN.credito_icms,'
      '         VEN.pis,'
      '         VEN.cofins,'
      '         VEN.loja,'
      '         VEN.codsubgrupo,'
      '         VEN.tipo,'
      '         VEN.codusuario,'
      '         VEN.origem,'
      '         VEN.destino,'
      '         VEN.produto,'
      '         VEN.codfilial,'
      '         VEN.aidf,'
      '         VEN.validade,'
      '         VEN.csosn,'
      '         VEN.nfce,'
      '         VEN.classificacao_fiscal,'
      '         VEN.base_sub,'
      '         VEN.icms_sub,'
      '         VEN.isentas_icms,'
      '         VEN.outras_icms,'
      '         VEN.gerou_sat,'
      '         VEN.numero_sat,'
      '         VEN.xitem,'
      '         VEN.xped,'
      '         VEN.devolvido,'
      '         VEN.ecf,'
      '         VEN.preco_custo,'
      '         VEN.idonline,'
      '         VEN.atb,'
      '         VEN.qtd_volume,'
      '         VEN.codprodutovolume,'
      'ma.nome, fo.nome, CLI.nome, fu.nome'
      'order by ma.nome, VEN.data, VEN.codigo')
    Params = <
      item
        DataType = ftUnknown
        Name = 'atbn'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'atbc000032'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end>
    Left = 760
    Top = 400
    ParamData = <
