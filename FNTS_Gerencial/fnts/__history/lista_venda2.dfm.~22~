  object qrvenda_produto: TZQuery
    Connection = frmmodulo.Conexao
    SQL.Strings = (
      'select'
      
        '  pro.produto, pro.unidade un, pro.precovenda preco_venda, case ' +
        'coalesce(ven.preco_custo,0) when 0 then'
      '    pro.precocusto else ven.preco_custo'
      
        '  end preco_custo, pro.codfornecedor, pro.codmarca, pro.aliquota' +
        ' ali, ven.*'
      'from'
      '  (select'
      '    codproduto,'
      '    unitario,'
      '    preco_custo,'
      '    avg(unitario) media_venda,'
      '    sum(qtde) total_qtde,'
      '    (unitario * sum(qtde)) - sum(desconto) total_geral'
      '  from'
      '    c000032'
      
        '    where movimento in (2,9,7) and movimento_estoque < 0 and  da' +
        'ta >= :datai and data <= :dataf'
      '  group by'
      '    codproduto,'
      '    unitario,'
      '    preco_custo'
      '  ) ven'
      '  inner join c000025 pro on pro.codigo = ven.codproduto'
      'where 1=1'
      'order by pro.codigo')
    Params = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end>
    Left = 458
    Top = 273
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end>
