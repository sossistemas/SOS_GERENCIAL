  object qrvenda_produto2: TZQuery
    Connection = frmmodulo.Conexao
    SQL.Strings = (
      'select  c000032.*, '
      '  case'
      '    when coalesce(c000032.nfce,'#39'N'#39') = '#39'S'#39' then'
      
        '      (Select FIRST 1 n.numero from nfce n where n.atb like :atb' +
        'n and n.cupom = c000032.numeronota)'
      '    when coalesce(c000032.gerou_sat,'#39'N'#39') = '#39'S'#39' then'
      '      c000032.numero_sat'
      
        '    when (coalesce(c000032.nfce,'#39'N'#39') <> '#39'S'#39') and (coalesce(c0000' +
        '32.gerou_sat,'#39'N'#39') <> '#39'S'#39') then'
      '      c000032.numeronota'
      '  end cupompdv,'
      
        '(select n.numero from nfce n where n.cupom = c000032.cupom_numer' +
        'o) as numero from c000032 ')
    Params = <
      item
        DataType = ftUnknown
        Name = 'atbn'
        ParamType = ptUnknown
      end>
    Left = 458
    Top = 217
    ParamData = <
      item
        DataType = ftUnknown
