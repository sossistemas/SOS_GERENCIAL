  object qryPesqLotes: TZQuery
    Connection = frmmodulo.Conexao
    Active = True
    SQL.Strings = (
      'select l.id,'
      '       l.descred,'
      '       l.data_inicio,'
      '       l.hora_inicio,'
      '       l.data_fim,'
      '       l.hora_fim,'
      '       l.status,'
      '       li.id idLI,'
      '       li.codproduto,'
      '       li.saldo,'
      '       li.fabricacaoprod,'
      '       li.precusto,'
      '       li.codfornecedor,'
      '       li.validadeproduto,'
      '       li.total_custo,'
      '       pr.produto,'
      '       pr.unidade,'
      '       fo.nome fornecedor'
      'from  lote l'
      'join lote_item li on li.id_lote = l.id'
      'join c000025 PR on pr.codigo = li.codproduto'
      'left join c000009 FO on FO.codigo = li.codfornecedor'
      'where 1 = 1'
      'and (:id is null or (l.id =:id))'
      
        'and ((:dtLini is null and :dtLfim is null) or (l.data_inicio bet' +
        'ween :dtLini and :dtLfim))'
      
        'and ((:dtVini is null and :dtVfim is null) or (l.data_fim betwee' +
        'n :dtVini and :dtVfim))'
      
        'and ((:dtVProd is null and :dtVProd is null) or (li.validadeprod' +
        'uto between :dtVProd and :dtVProd))'
      'and (:codp is null or (li.codproduto =:codp))'
      'and (:codfo is null or (li.codfornecedor =:codfo))'
      'order by li.id')
    Params = <
      item
        DataType = ftUnknown
        Name = 'id'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtLini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtLfim'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVfim'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVProd'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'codp'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'codfo'
        ParamType = ptUnknown
      end>
    Left = 170
    Top = 281
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'id'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtLini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtLfim'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVini'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVfim'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dtVProd'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'codp'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'codfo'
        ParamType = ptUnknown
      end>
